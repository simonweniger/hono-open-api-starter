{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": "./",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"paths": {
			"@/*": ["./src/*"]
		},
		"typeRoots": ["./node_modules/@types"],
		"types": ["node"],
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"type": "module",
	"tsc-alias": {
		"resolveFullPaths": true
	}
}
